{
  "name": "awayhugo",
  "version": "1.0.0",
  "description": "Build process for awayhugo",
  "devDependencies": {
    "autoprefixer": "^9.1.0",
    "cssnano": "^4.0.5",
    "fancy-log": "^1.3.2",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-postcss": "^7.0.1",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-uglify": "^3.0.1",
    "node-sass": "^5.0.0",
    "npm": "^6.3.0",
    "precss": "^3.1.2",
    "prettier": "1.13.7",
    "typescript": "4.1"
  },
  "scripts": {
    "tsc": "node_modules/.bin/tsc",
    "dev": "node_modules/.bin/gulp watch & tsc --watch & cd ../.. && hugo server --watch --ignoreCache --config config.dev.toml",
    "prod": "node_modules/.bin/gulp watch & cd ../.. && hugo server --watch",
    "build": "node_modules/.bin/gulp build & cd ../.. && hugo server --watch",
    "vendor": "node_modules/.bin/gulp vendor",
    "prettier": "node_modules/.bin/prettier --list-different --write 'src/styles/**/*.{scss,css}'"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {}
}
